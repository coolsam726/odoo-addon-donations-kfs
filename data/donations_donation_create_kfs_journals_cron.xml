<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="donation_donation_create_kfs_journals_cron" forcecreate='True' model="ir.cron">
            <field name="name">Donations Records: Create KFS Journals</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(minutes=10)).strftime('%Y-%m-%d %H:%M:00')"/>
            <field name="model_id" ref="model_donations_donation"/>
            <field name="state">code</field>
            <field name="code">model._cron_create_kfs_journals()</field>
        </record>

    </data>
</odoo>